 <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-info">
              <div class="inner">
                <h3>{{ lengthEtudiant | length }}</h3>

                <p>Etudiants inscrits </p>
              </div>
              <div class="icon">
                <i class="ion ion-person-add"></i>
              </div>
              <a href="{{ path('liste_etudiants') }}" class="small-box-footer">voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-success">
              <div class="inner">
                <h3>{{ lengthClasse | length }}</h3>
                <p>Classes</p>
              </div>
              <div class="icon">
                <i class="ion ion-stats-bars"></i>
              </div>
              <a href="{{ path('liste_classes') }}" class="small-box-footer"> voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
            
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-warning">
              <div class="inner">
                <h3>{{ lengthUser | length }}</h3>

                <p>Utilisateurs</p>
              </div>
              <div class="icon">
                <i class="ion ion-person-stalker"></i>
              </div>
              <a href="{{ path('liste_users') }}" class="small-box-footer"> voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-danger">
              <div class="inner">
                <h3>{{ lengthProfs | length }}</h3>

                <p>Professeurs</p>
              </div>
              <div class="icon">
                <i class="ion ion-pie-graph"></i>
              </div>
              <a href="{{ path('liste_prof') }}" class="small-box-footer"> voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
          </div>
          <!-- ./col -->
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
          <!-- Left col -->
          <div class="col-md-12">
            <!-- Custom tabs (Charts with tabs)-->
            

            <!-- DIRECT CHAT -->
            <div class="card direct-chat direct-chat-primary">
              <div class="card-header">
                <h3 class="card-title">RECEMMENT INSCRITS</h3>

                <div class="card-tools">
                  <button type="button" class="btn btn-tool" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                  </button>
                 
                    <a href="{{path('app_home')}}"><i class="fa fa-undo" aria-hidden="true"></i></a>

                  <button type="button" class="btn btn-tool" data-card-widget="remove">
                    <i class="fas fa-times"></i>
                  </button>
                </div>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                {% if app.request.method == 'POST' %}

                    {% if search | length == 0 %}

                        <div class="row text-center">
                            <h3>Aucun résultat pour votre recherche</h3>
                        </div>

                    {% else %}

                              <table class="table table-hover mb-5">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">Nom</th>
                                        <th scope="col">Prénoms</th>
                                        <th scope="col">Matricule</th>
                                        <th scope="col">Classe</th>
                                        <th scope="col">Photo</th>
                    
                                    </tr>
                                </thead>
                                <tbody>
                                {% for search in search %}
                       
                                <tr>
                                    <th>{{ search.nom | upper}}</th>
                                    <td>{{ search.prenoms | upper}}</td>
                                    <td>{{ search.matricule | upper}}</td>
                                    <td>{{ search.classe | upper}}</td>
                                
                                    <td><img src="{{ vich_uploader_asset(search, 'imageFile') }}" class="image-circle" height="35" width="35"></td>
                                </tr>

                        {% endfor %}

                         </tbody>
                    </table>
                    {% endif %}
                {% else %}

                    <table class="table table-hover mb-5">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Nom</th>
                            <th scope="col">Prénoms</th>
                            <th scope="col">Matricule</th>
                            <th scope="col">Classe</th>
                        
                            <th class="ms-5">Photo</th>
        
                        </tr>
                    </thead>
                    <tbody>

                        {% for etudiant in etudiant %}

                            <tr>
                                <th>{{ etudiant.etud.nom | upper}}</th>
                                <td>{{ etudiant.etud.prenoms | upper}}</td>
                                <td>{{ etudiant.etud.matricule | upper}}</td>
                                <td>{{ etudiant.etud.classe | upper}}</td>
                                <td><img src="{{ asset('/uploads/'~ etudiant.etud.imageFile) }}" class="image-circle" height="35" width="35"></td>
                            </tr>
                    
                        {% endfor %}
                        

                        
                        
                    </tbody>
                </table>
            
                {% endif %}

              </div>
              <!-- /.card-body -->

            </div>
          </div>

        </div>
        <!-- /.row (main row) -->
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->