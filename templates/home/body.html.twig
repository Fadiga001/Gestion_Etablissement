 <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-info">
              <div class="inner">
                <h3>{{ lengthEtudiant | length }}</h3>

                <p>Etudiants inscrits</p>
              </div>
              <div class="icon">
                <i class="ion ion-person-add"></i>
              </div>
              <a href="{{ path('liste_etudiants') }}" class="small-box-footer">voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-success">
              <div class="inner">
                <h3>{{ lengthClasse | length }}</h3>
                <p>Classes</p>
              </div>
              <div class="icon">
                <i class="ion ion-stats-bars"></i>
              </div>
              <a href="{{ path('liste_classes') }}" class="small-box-footer"> voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
            
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-warning">
              <div class="inner">
                <h3>{{ lengthUser | length }}</h3>

                <p>Utilisateurs</p>
              </div>
              <div class="icon">
                <i class="ion ion-person-stalker"></i>
              </div>
              <a href="{{ path('liste_users') }}" class="small-box-footer"> voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
          </div>
          <!-- ./col -->
          <div class="col-lg-3 col-6">
            <!-- small box -->
            <div class="small-box bg-danger">
              <div class="inner">
                <h3>{{ lengthProfs | length }}</h3>

                <p>Professeurs</p>
              </div>
              <div class="icon">
                <i class="ion ion-pie-graph"></i>
              </div>
              <a href="{{ path('liste_prof') }}" class="small-box-footer"> voir plus <i class="fa fa-eye" aria-hidden="true"></i></a>
            </div>
          </div>
          <!-- ./col -->
        </div>
        <!-- /.row -->
        <!-- Main row -->
        <div class="row">
          <!-- Left col -->
          <div class="col-md-6">
            <!-- Custom tabs (Charts with tabs)-->
            

            <!-- DIRECT CHAT -->
            <div class="card direct-chat direct-chat-primary">
              <div class="card-header">
                <h3 class="card-title">RECEMMENT INSCRITS</h3>

                <div class="card-tools">
                  <button type="button" class="btn btn-tool" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                  </button>
                 
                    <a href="{{path('app_home')}}"><i class="fa fa-undo" aria-hidden="true"></i></a>

                  <button type="button" class="btn btn-tool" data-card-widget="remove">
                    <i class="fas fa-times"></i>
                  </button>
                </div>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                {% if app.request.method == 'POST' %}

                    {% if search | length == 0 %}

                        <div class="row text-center">
                            <h3>Aucun résultat pour votre recherche</h3>
                        </div>

                    {% else %}

                              <table class="table table-hover mb-5">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">Nom</th>
                                        <th scope="col">Prénoms</th>
                                        <th scope="col">Matricule</th>
                                        <th scope="col">Classe</th>
                                        <th scope="col">Photo</th>
                    
                                    </tr>
                                </thead>
                                <tbody>
                                {% for search in search %}
                       
                                <tr>
                                    <th>{{ search.nom | upper}}</th>
                                    <td>{{ search.prenoms | upper}}</td>
                                    <td>{{ search.matricule | upper}}</td>
                                    <td>{{ search.classe | upper}}  {{ search.filieres | upper}}</td>
                                
                                    <td><img src="{{ vich_uploader_asset(search, 'imageFile') }}" class="image-circle" height="35" width="35"></td>
                                </tr>

                        {% endfor %}

                         </tbody>
                    </table>
                    {% endif %}
                {% else %}

                    <table class="table table-hover mb-5">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Nom</th>
                            <th scope="col">Prénoms</th>
                            <th scope="col">Matricule</th>
                            <th scope="col">Classe</th>
                        
                            <th class="ms-5">Photo</th>
        
                        </tr>
                    </thead>
                    <tbody>

                        {% for etudiant in etudiant %}

                            <tr>
                                <th>{{ etudiant.nom | upper}}</th>
                                <td>{{ etudiant.prenoms | upper}}</td>
                                <td>{{ etudiant.matricule | upper}}</td>
                                <td>{{ etudiant.classe | upper}}  {{ etudiant.filieres | upper}}</td>
                                <td><img src="{{ vich_uploader_asset(etudiant, 'imageFile') }}" class="image-circle" height="35" width="35"></td>
                            </tr>
                    
                        {% endfor %}
                        

                        
                        
                    </tbody>
                </table>
            
                {% endif %}

              </div>
              <!-- /.card-body -->

            </div>
          </div>

          <div class="col-md-6">
            <!-- Custom tabs (Charts with tabs)-->  
            <!-- Calendar -->
            <div class="card bg-gradient-success">
              <div class="card-header border-0">

                <h3 class="card-title">
                  <i class="fa fa-camera-retro" aria-hidden="true"></i>
                  GALERIE
                </h3>
                <!-- tools card -->
                <div class="card-tools">
                  <!-- button with a dropdown -->
                  <div class="btn-group">
                    <button type="button" class="btn btn-success btn-sm dropdown-toggle" data-toggle="dropdown" data-offset="-52">
                      <i class="fas fa-bars"></i>
                    </button>
                    <div class="dropdown-menu" role="menu">
                      <a href="{{ path('ajouter_image')}}" class="dropdown-item">Ajouter une photo</a>
                      <a href="{{ path('liste_photo')}}" class="dropdown-item">Toutes les photos</a>
                      
                    </div>
                  </div>
                  <button type="button" class="btn btn-success btn-sm" data-card-widget="collapse">
                    <i class="fas fa-minus"></i>
                  </button>
                  <button type="button" class="btn btn-success btn-sm" data-card-widget="remove">
                    <i class="fas fa-times"></i>
                  </button>
                </div>
                <!-- /. tools -->
              </div>
              <!-- /.card-header -->
              <div class="card-body pt-0">
                  <div class="container">
                      <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
                        <div class="carousel-indicators">
                          <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                          <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                          <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
                        </div>
                          <div class="carousel-inner">
                           {% for allImage in allImage %}
                            <div class="carousel-item active">
                              <img src="{{ vich_uploader_asset(allImage, 'imageFile') }}" class="d-block w-100" alt="...">
                              
                            </div>
                            {% endfor %}
                          </div>
                        
                        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
                          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                          <span class="visually-hidden">Previous</span>
                        </button>
                        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
                          <span class="carousel-control-next-icon" aria-hidden="true"></span>
                          <span class="visually-hidden">Next</span>
                        </button>
                      </div>
                  </div>
                <div id="calendar" style="width: 100%"></div>
              </div>
              <!-- /.card-body -->
            </div>
            <!-- /.card -->
          </div>
          <!-- right col -->
        </div>
        <!-- /.row (main row) -->
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->