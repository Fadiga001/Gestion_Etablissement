    {% for label, messages in app.flashes %}
        <div class=" alert alert-{{ label }}">

            {% for message  in messages %}
                {{ message }}
            {% endfor %}
            <button type="button" class="close text-light" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            
        </div>
        
    {% endfor %}